#!/bin/bash

python="/home/will/.virtualenvs/deap/bin/python"
$python ./setup.py build_ext --inplace

ngen=60000
log_freq=500

for ((seed=0; seed < 32; seed++))
do
  output_dir="raw_results/motor-coercion/0.0.4/3-4-6-5/seed-$seed"
  tasks="tasks/3-4-6-5.yml"
  params="params.yml"
  $python ./evolve.py $output_dir $tasks $params -n $ngen -s $seed -l $log_freq &
done
